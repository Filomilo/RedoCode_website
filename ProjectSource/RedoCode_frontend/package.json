{
  "name": "yarn_ts_vue",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "test:dev:unit": "vitest",
    "test:dev:e2e": "start-server-and-test 'vite dev --port 4173' http://localhost:4173 'cypress run --e2e -C config/cypress-dev.config.ts'",
    "test:dev:e2e:dev": "start-server-and-test 'vite dev --port 4173' http://localhost:4173 'cypress open --e2e -C config/cypress-dev.config.ts  '",
    "test:prod:e2e": "start-server-and-test preview http://localhost:4173 'cypress run --e2e -C config/cypress-prod.config.ts  '",
    "test:prod:e2e:dev": "start-server-and-test 'vite preview --port 4173' http://localhost:4173 'cypress open --e2e -C config/cypress-prod.config.ts  '",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "predeploy": "yarn build",
    "deploy": "gh-pages -d dist",
    "serve": "serve -s dist"
  },
  "dependencies": {
    "@guolao/vue-monaco-editor": "^1.5.1",
    "@stomp/stompjs": "^7.0.0",
    "agent-base": "^7.1.1",
    "bootstrap": "^5.3.2",
    "chart.js": "^4.4.2",
    "gh-pages": "^6.1.1",
    "markdown-it": "^14.1.0",
    "pinia": "^2.1.7",
    "primevue": "^3.48.1",
    "v-dropdown-menu": "^2.0.4",
    "v-hotkey": "^0.9.0",
    "vue": "^3.3.11",
    "vue-router": "^4.2.5",
    "vue-slider-component": "^4.1.0-beta.7",
    "vue3-easy-data-table": "^1.5.47"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.3.3",
    "@tsconfig/node18": "^18.2.2",
    "@types/bootstrap": "^5",
    "@types/gh-pages": "^6",
    "@types/jsdom": "^21.1.6",
    "@types/markdown-it": "^13.0.7",
    "@types/node": "^18.19.25",
    "@vitejs/plugin-vue": "^4.5.2",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.3",
    "@vue/tsconfig": "^0.5.0",
    "cypress": "^13.6.3",
    "eslint": "^8.49.0",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-vue": "^9.17.0",
    "jsdom": "^23.0.1",
    "miragejs": "^0.1.48",
    "npm-run-all2": "^6.1.1",
    "prettier": "^3.0.3",
    "sass": "^1.70.0",
    "start-server-and-test": "^2.0.3",
    "typescript": "~5.3.0",
    "vite": "^5.2.7",
    "vitest": "^1.0.4",
    "vue-tsc": "^1.8.25"
  },
  "packageManager": "yarn@4.0.2"
}
